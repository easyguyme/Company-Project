{
  "$schema": "http://json-schema.org/draft-03/schema",
  "type": "object",
  "properties": {
    "accesstoken": {
      "type": "string",
      "required": true,
      "description": "用户token（放在querystring中）"
    },
    "productId": {
      "type": "string",
      "required": true,
      "description": "productId"
    },
    "createdAt": {
      "type": "timestamp",
      "required": true,
      "description": "时间戳创建时间"
    }
  }
}