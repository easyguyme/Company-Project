{
  "$schema": "http://json-schema.org/draft-03/schema",
  "type": "object",
  "properties": {
    "accesstoken": {
      "type": "string",
      "required": false,
      "description": "用户token（放在querystring中） type为'updateCompanyInfo'和'exchange'时必须"
    },
    "type": {
      "type": "string",
      "required": true,
      "description": "渠道类型(weibo/alipay)"
    },
    "weiboToken": {
      "type": "string",
      "required": false,
      "description": "channelType为weibo时必填"
    }
  }
}